# Production Environment
# Copy this file to production.tfvars and fill in actual values
# NEVER commit production.tfvars to the repository

environment = "production"

# DB setting - use a strong, unique password (different from UAT!)
db_password = "CHANGE_ME_use_a_strong_password"

# JWT - use a long random string (min 32 chars, different from UAT!)
jwt_secret = "CHANGE_ME_use_a_random_secret_key"

# AWS Region
aws_region = "ap-southeast-1"
