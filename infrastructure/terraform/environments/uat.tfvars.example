# UAT Environment
# Copy this file to uat.tfvars and fill in actual values
# NEVER commit uat.tfvars to the repository

environment = "uat"

# DB setting - use a strong, unique password
db_password = "CHANGE_ME_use_a_strong_password"

# JWT - use a long random string (min 32 chars)
jwt_secret = "CHANGE_ME_use_a_random_secret_key"

# AWS Region
aws_region = "ap-southeast-1"
