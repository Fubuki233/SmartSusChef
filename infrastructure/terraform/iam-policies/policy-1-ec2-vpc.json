{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ec2:*Vpc*",
                "ec2:*Subnet*",
                "ec2:*Gateway*",
                "ec2:*Route*",
                "ec2:*SecurityGroup*",
                "ec2:*NetworkAcl*",
                "ec2:*Address*",
                "ec2:*Tags*",
                "ec2:Describe*"
            ],
            "Resource": "*"
        }
    ]
}